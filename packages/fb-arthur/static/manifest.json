{
  "manifest_version": 2,
  "name": "FB Arthur",
  "version": "0.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/inject.js"]
    }
  ],
  "web_accessible_resources": ["js/content-script.js"],
  "permissions": ["activeTab"]
}
